<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">
<!-- the theme applied to the application or activity -->
<style name="NamakTheme"
    parent="@style/Theme.AppCompat.Light.DarkActionBar">
    <!--<item name="android:actionMenuTextColor">@color/actionbar_text</item>-->
    <item name="actionBarStyle">@style/NamakActionBar</item>
    <item name="android:actionBarStyle" tools:ignore="NewApi">@style/NamakActionBar</item>
    <item name="android:activatedBackgroundIndicator">@drawable/list_background</item>
</style>
<style name="NamakActionBar"
    parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">
    <item name="background">@color/DarkestBlue</item>
    <item name="android:background" tools:ignore="NewApi">@color/DarkestBlue</item>
    <item name="titleTextStyle">@style/NamakActionBarTitleText</item>
    <item name="android:titleTextStyle" tools:ignore="NewApi">@style/NamakActionBarTitleText</item>
    <!--<item name="android:height">@dimen/app_head_height</item>-->
</style>
<style name="NamakActionBarTitleText"
    parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
    <!--<item name="textColor">@color/LighterBlue</item>-->
    <item name="android:textColor" tools:ignore="NewApi">@color/LighterBlue</item>
</style>
</resources>